-- -----------------------------------------------------
-- Table `SOEN341`.`book`              
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `SOEN341`.`book` (
  `Title` VARCHAR(255) NULL,
  `Author` VARCHAR(45) NULL,
  `Format` VARCHAR(45) NULL,
  `Pages` INT NULL,
  `Publisher` VARCHAR(255) NULL,
  `Language` VARCHAR(45) NULL,
  `ISBN-10` VARCHAR(255) NULL,
  `ISBN-13` VARCHAR(255) NULL,
  PRIMARY KEY (`ISBN-10`))
ENGINE = InnoDB;







