SET @OLD_UNIQUE_CHECKS=@@UNIQUE_CHECKS, UNIQUE_CHECKS=0;
SET @OLD_FOREIGN_KEY_CHECKS=@@FOREIGN_KEY_CHECKS, FOREIGN_KEY_CHECKS=0;
SET @OLD_SQL_MODE=@@SQL_MODE, SQL_MODE='ONLY_FULL_GROUP_BY,STRICT_TRANS_TABLES,NO_ZERO_IN_DATE,NO_ZERO_DATE,ERROR_FOR_DIVISION_BY_ZERO,NO_ENGINE_SUBSTITUTION';

-- -----------------------------------------------------
-- Schema SOEN341
-- -----------------------------------------------------

-- -----------------------------------------------------
-- Schema SOEN341
-- -----------------------------------------------------
CREATE SCHEMA IF NOT EXISTS `SOEN341` DEFAULT CHARACTER SET utf8 ;
USE `SOEN341` ;

-- -----------------------------------------------------
-- Table `SOEN341`.`book`              
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `SOEN341`.`book` (
  `Title` VARCHAR(255) NULL,
  `Author` VARCHAR(45) NULL,
  `Format` VARCHAR(45) NULL,
  `Pages` INT NULL,
  `Publisher` VARCHAR(255) NULL,
  `Language` VARCHAR(45) NULL,
  `ISBN-10` VARCHAR(255) NULL,
  `ISBN-13` VARCHAR(255) NULL,
  PRIMARY KEY (`ISBN-10`))
ENGINE = InnoDB;

-- -----------------------------------------------------
-- Table `SOEN341`.`movie`               
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `SOEN341`.`movie` (
  `Title` VARCHAR(255) NULL,
  `Director` VARCHAR(45) NULL,
  `Producers` VARCHAR(255) NULL,
  `Actors` VARCHAR(255) NULL,
  `Language` VARCHAR(45) NULL,
  `Subtitles` VARCHAR(45) NULL,
  `Dubbed` VARCHAR(45) NULL,
  `Release Date` VARCHAR(45) NULL,
  `Run Time` VARCHAR(100) NULL,
  PRIMARY KEY (`Title`))
ENGINE = InnoDB;

-- -----------------------------------------------------
-- Table `SOEN341`.`music`  
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `SOEN341`.`music` (
  `Type` VARCHAR(45) NULL,
  `Title` VARCHAR(255) NULL,
  `Artist` VARCHAR(100) NULL,
  `Label` VARCHAR(45) NULL,
  `Release Date` VARCHAR(100) NULL,
  `ASIN` VARCHAR(100) NULL,
  PRIMARY KEY (`Title`))
ENGINE = InnoDB;

-- -----------------------------------------------------
-- Table `SOEN341`.`magazine`                                 
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `SOEN341`.`magazine` (
  `Title` VARCHAR(255) NULL,
  `Publisher` VARCHAR(255) NULL,
  `Language` VARCHAR(255) NULL,
  `ISBN-10` VARCHAR(255) NULL,
  `ISBN-13` VARCHAR(255) NULL,
  PRIMARY KEY (`ISBN-10`))
ENGINE = InnoDB;



-- -----------------------------------------------------
-- Table `SOEN341`.`Administrator`
-- -----------------------------------------------------
CREATE TABLE IF NOT EXISTS `SOEN341`.`Administrator` (
  `UserName` VARCHAR(45) NULL,
  `Password` VARCHAR(45) NULL)
ENGINE = InnoDB;


SET SQL_MODE=@OLD_SQL_MODE;
SET FOREIGN_KEY_CHECKS=@OLD_FOREIGN_KEY_CHECKS;
SET UNIQUE_CHECKS=@OLD_UNIQUE_CHECKS;



